{
  "version": 2,
  "builds": [
    {
      "src": "XtraROMs/*",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/media/(.*)",
      "dest": "media/$1"
    },
    {
      "src": "/static/(.*)",
      "dest": "static/$1"
    },
    {
      "src": "/.*",
      "dest": "XtraROMs/wsgi.py"
    }
  ],
  "env": {
    "DJANGO_SETTINGS_MODULE": "XtraROMs.settings"
  }
}
