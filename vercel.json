{
  "version": 2,
  "builds": [
    {
      "src": "XtraROMs/asgi.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "static/$1"
    },
    {
      "src": "/media/(.*)",
      "dest": "media/$1"
    },
    {
      "src": "/(.*)",
      "dest": "XtraROMs/asgi.py",
      "headers": {
        "content-type": "application/json"
      }
    }
  ],
  "env": {
    "DJANGO_SETTINGS_MODULE": "XtraROMs.settings"
  }
}
