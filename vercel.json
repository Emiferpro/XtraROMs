{
  "version": 2,
  "builds": [
    {
      "src": "app/**/*.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/app/(.*)",
      "dest": "/app/\\1"
    },
    {
      "src": "/static/(.*)",
      "dest": "/app/static/\\1"
    },
    {
      "src": "/media/(.*)",
      "dest": "/app/media/\\1"
    },
    {
      "src": "/favicon.ico",
      "dest": "/app/static/favicon.ico"
    },
    {
      "src": "/(.*)",
      "dest": "/app/\\1"
    }
  ]
}
